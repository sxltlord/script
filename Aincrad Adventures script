local page6 = venyx:addPage("Inf money", 5012544693)
local section1 = page6:addSection("inf money")

section1:addToggle("Inf Money", nil, function(state)
    getgenv().toggle = state 

while wait(0.1) do
        if getgenv().toggle == true then
            local args = {
                [1] = "MidnightCloak", 
                [2] = "Armor", 
                [3] = "None"
            }
               while true do 
                game:GetService("ReplicatedStorage").Inventory.EquipArmor:FireServer(unpack(args))
                wait (0.1)
                game:GetService("ReplicatedStorage").Quest.sellarmor:FireServer()
                end
        end
    end
end)

local page7 = venyx:addPage("Artifact", 5012544693)
local section1 = page7:addSection("Artifact - Must use in dungeon only! ")

local Artifact = {
    "Eightgates",
    "Ultrainstinct",
}

section1:addDropdown("Artifact", Artifact, function(text)
    getgenv().Artifact = text
end)

section1:addButton("Change!", function()
    local args = {
        [1] = getgenv().Dagger,
        [2] = "Artifact",
        [3] = "None"
    }

game:GetService("ReplicatedStorage").Inventory.EquipItem:FireServer()
